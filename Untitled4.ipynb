        "class sq_sum:\n",
        "    def __init__(self,x,y,z):\n",
        "        self.x=x\n",
        "        self.y=y\n",
        "        self.z=z\n",
        "    def square_sum(self):\n",
        "        return ((self.x*self.x)+(self.y*self.y)+(self.z*self.z))\n",
        "w=sq_sum(1,3,5)\n",
        "print(w.square_sum())"
      ]
